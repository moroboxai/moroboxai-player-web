  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.1.0",
    "chai": "^4.3.6",
    "mocha": "^9.2.2",
    "moroboxai-game-sdk": "^0.1.0-alpha.7",
    "moroboxai-player-sdk": "^0.1.0-alpha.1",
    "@moroboxai-games/pixijs-template": "^0.1.0-alpha.1",
